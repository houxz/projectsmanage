		<property name="label" value="人员信息管理" />
		<property name="roleSet">
			<set>
				<value type="com.emg.poiwebeditor.common.RoleType">ROLE_SUPERADMIN</value>
				<value type="com.emg.poiwebeditor.common.RoleType">ROLE_YANFAADMIN</value>
				<value type="com.emg.poiwebeditor.common.RoleType">ROLE_ADMIN</value>
