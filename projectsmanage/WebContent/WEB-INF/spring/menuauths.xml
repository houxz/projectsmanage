<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="menuConfig" class="com.emg.projectsmanage.config.MenuConfig">
		<property name="menus">
			<set>
				<ref bean="usersmanage" />
				<ref bean="processesmanage" />
				<ref bean="worktasks" />
				<ref bean="projectsprocess" />
				<ref bean="errorsmanage" />
				<ref bean="itemsetmanage" />
				<ref bean="errorsetmanage" />
				<ref bean="iteminfo" />
				<ref bean="systemsets" />
			</set>
		</property>
	</bean>

	<bean id="systemsets" class="com.emg.projectsmanage.auth.MenuAuthModel">
		<property name="code" value="990" />
		<property name="url" value="/systemsets.web" />
		<property name="label" value="系统配置" />
		<property name="roleSet">
			<set>
				<value type="com.emg.projectsmanage.common.RoleType">ROLE_POIVIDEOEDIT</value>
			</set>
		</property>
		<property name="enabled" value="true" />
	</bean>
	
	<bean id="usersmanage" class="com.emg.projectsmanage.auth.MenuAuthModel">
		<property name="code" value="999" />
		<property name="url" value="/usersmanage.web" />
		<property name="label" value="人员信息管理" />
		<property name="roleSet">
			<set>
				<value type="com.emg.projectsmanage.common.RoleType">ROLE_SUPERADMIN</value>
				<value type="com.emg.projectsmanage.common.RoleType">ROLE_ADMIN</value>
			</set>
		</property>
		<property name="enabled" value="true" />
	</bean>
	
	<bean id="skillsmanage" class="com.emg.projectsmanage.auth.MenuAuthModel">
