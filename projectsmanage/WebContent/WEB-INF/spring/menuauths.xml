		<property name="roleSet">
			<set>
				<value type="com.emg.projectsmanage.common.RoleType">ROLE_POIVIDEOEDIT</value>
			</set>
		</property>
		<property name="enabled" value="false" />
	</bean>
	
	<bean id="processesmanage" class="com.emg.projectsmanage.auth.MenuAuthModel">
		<property name="code" value="1001" />
		<property name="url" value="/processesmanage.web" />
		<property name="label" value="项目管理" />
		<property name="roleSet">
			<set>
				<value type="com.emg.projectsmanage.common.RoleType">ROLE_POIVIDEOEDIT</value>
			</set>
		</property>
		<property name="enabled" value="true" />
	</bean>
	
	<bean id="projectsmanage" class="com.emg.projectsmanage.auth.MenuAuthModel">
		<property name="code" value="1002" />
