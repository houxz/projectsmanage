	</bean>
	
	<bean id="worktasks" class="com.emg.projectsmanage.auth.MenuAuthModel">
		<property name="code" value="1003" />
		<property name="url" value="/worktasks.web" />
		<property name="label" value="作业任务" />
		<property name="roleSet">
			<set>
				<value type="com.emg.projectsmanage.common.RoleType">ROLE_POIVIDEOEDIT</value>
				<value type="com.emg.projectsmanage.common.RoleType">ROLE_WORKER</value>
				<value type="com.emg.projectsmanage.common.RoleType">ROLE_CHECKER</value>
			</set>
		</property>
		<property name="enabled" value="true" />
